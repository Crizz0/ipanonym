{
	"name": "crizzo/ipanonym",
	"type": "phpbb-extension",
	"description": "This phpBB extension will anonymise the user's IP in posts, logs, sessions etc.",
	"homepage": "https://github.com/Crizz0/ipanonym",
	"version": "1.0.2",
	"time": "2020-08-03",
	"license": "GPL-2.0-only",
	"authors": [{
			"name": "Christian Schnegelberger",
			"email": "kontakt@crizzo.de",
			"homepage": "https://www.crizzo.de",
			"role": "Developer/Founder"
		}],
	"require": {
		"php": ">=5.4.7"
	},
	"extra": {
		"display-name": "IP anonymised",
		"soft-require": {
			"phpbb/phpbb": ">=3.2.4, <4.0.0@dev"
		},
		"version-check": {
			"host": "www.crizzo.de",
			"directory": "/phpbbextversion",
			"filename": "phpbb-ipanonym.json"
		}
	}
}
